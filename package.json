{
  "name": "@blablacar/ui-library",
  "version": "0.2.0",
  "description": "React component library",
  "main": "build/index.js",
  "homepage": "https://blablacar.github.io/ui-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:blablacar/ui-library.git"
  },
  "scripts": {
    "build": "rimraf build && npm-run-all build:*",
    "build:js": "tsc && babel build/src -x \".jsx\",\".js\" --ignore \"**/*.unit.jsx\",\"**/story.jsx\" --out-dir build && rimraf build/src && rimraf build/stories",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "stylelint": "grep -r -l --include=*.css \"\" ./src | xargs stylelint",
    "lint": "npm-run-all lint:*",
    "lint:js": "git diff --name-only --cached --diff-filter=ACMRTUXB -- \"src/*.ts\" \"src/*.tsx\" | xargs tslint",
    "lint:css": "git diff --name-only --cached --diff-filter=ACMRTUXB -- \"src/*.css\" | xargs stylelint",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "watch": "npm-run-all --parallel watch:*",
    "watch:test": "npm test -- --watch --onlyChanged",
    "watch:css": "onchange \"**/*.css\" -- npm run stylelint",
    "watch:js": "onchange \"**/*.ts\" \"**/*.tsx\" -- npm run tslint",
    "kirk": "node ./bin/kirkInit.js",
    "start": "export NODE_PATH=src && start-storybook -p 9001 -c .storybook",
    "check-node": "bla node",
    "check-package": "bla check",
    "bla": "bla",
    "prepublishOnly": "npm run tslint && npm run stylelint && npm run test && npm run build",
    "storybook-build": "export NODE_PATH=src && build-storybook -c .storybook -o storybook-build",
    "storybook-deploy": "npm run storybook-build && gh-pages -d storybook-build"
  },
  "author": "Blablacar front-end team",
  "keywords": [
    "blablacar",
    "design-system",
    "lego",
    "kirk",
    "nodeJS",
    "react",
    "es6"
  ],
  "license": "MIT",
  "devDependencies": {
    "@blablacar/cli": "0.1.2",
    "@storybook/addon-actions": "3.4.4",
    "@storybook/addon-info": "3.4.4",
    "@storybook/addon-knobs": "3.4.4",
    "@storybook/addons": "3.4.4",
    "@storybook/react": "3.4.4",
    "@types/enzyme": "3.1.10",
    "@types/jest": "22.2.3",
    "@types/lodash.debounce": "4.0.3",
    "@types/lodash.padstart": "4.6.3",
    "@types/react": "16.3.14",
    "@types/react-dates": "16.0.6",
    "@types/react-dom": "16.0.5",
    "@types/storybook__react": "3.0.7",
    "@types/styled-jsx": "2.2.3",
    "ansi": "0.3.1",
    "awesome-typescript-loader": "4.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "22.4.3",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-resolver": "1.1.0",
    "caniuse-db": "1.0.30000840",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "gh-pages": "1.1.0",
    "glob": "7.1.2",
    "inquirer": "5.2.0",
    "jest": "22.4.3",
    "jsdom": "11.10.0",
    "jsdom-global": "3.0.2",
    "lodash.camelcase": "4.3.0",
    "lodash.template": "4.4.0",
    "npm-run-all": "4.1.3",
    "onchange": "4.0.0",
    "path": "0.12.7",
    "pre-commit": "1.2.2",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.3.2",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "stylelint": "9.2.0",
    "ts-jest": "22.4.6",
    "ts-loader": "3.5.0",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@types/react-transition-group": "2.0.9",
    "classcat": "3.1.2",
    "exenv": "1.2.2",
    "lodash.debounce": "4.0.8",
    "lodash.kebabcase": "4.1.1",
    "lodash.padstart": "4.6.1",
    "moment": "2.22.1",
    "react": "16.3.2",
    "react-addons-shallow-compare": "15.6.2",
    "react-dates": "16.7.0",
    "react-dom": "16.3.2",
    "react-transition-group": "2.3.1",
    "styled-jsx": "2.2.6",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.8.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/bin/jest.preprocessor.js",
      "jestScript.js|utils.js": "<rootDir>/node_modules/babel-jest"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "collectCoverageFrom": [
      "src/**/index.{ts, js}"
    ],
    "testRegex": "(src/.*.unit.(js|jsx|tsx)$)",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "setupTestFrameworkScriptFile": "./bin/jestScript"
  },
  "pre-commit": [
    "check-node",
    "check-package",
    "lint",
    "test"
  ]
}
